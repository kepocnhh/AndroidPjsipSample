<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="android.pjsip.sample">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        android:name=".App"
        android:icon="@mipmap/ic_launcher"
        android:label="AndroidPjsipSample"
        >
        <activity android:name=".v2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".v2.CallActivity"/>
<!--        <activity android:name=".v1.DialActivity"/>-->
<!--        <activity android:name=".v1.RegistrationActivity"/>-->
        <service android:name=".v2.CallService"/>
    </application>
</manifest>
